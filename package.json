{
  "name": "labelbaker-creative-editor",
  "version": "1.0.3",
  "description": "A powerful creative editor for labels and designs",
  "author": "Prappo",
  "license": "MIT",
  "keywords": [
    "editor",
    "label",
    "design",
    "creative",
    "canvas",
    "konva"
  ],
  "main": "dist/lib/editor.cjs.js",
  "module": "dist/lib/editor.es.js",
  "unpkg": "dist/lib/editor.umd.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib/editor.es.js",
      "require": "./dist/lib/editor.cjs.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:lib": "vite build --config vite.lib.config.js",
    "preview": "vite preview",
    "dbuild": "vite build --debug",
    "visualizer": "npx vite-bundle-visualizer",
    "prepublishOnly": "npm run build:lib"
  },
  "dependencies": {
    "@cloudfour/simple-svg-placeholder": "^1.1.0",
    "@fatal_error/libs_change": "^1.0.0",
    "@formester/universal-font-picker": "^0.0.9",
    "@headlessui/vue": "^1.7.8",
    "@heroicons/vue": "^2.0.14",
    "crypto-js": "^4.1.1",
    "dateformat": "^5.0.3",
    "flowbite": "^1.6.3",
    "fontfaceobserver": "^2.3.0",
    "fuse.js": "^6.6.2",
    "get-svg-colors-browser": "^2.0.7",
    "jsbarcode": "^3.11.5",
    "jspdf": "^2.5.1",
    "konva": "^8.4.2",
    "qrcode": "^1.5.1",
    "vue": "^3.2.47",
    "vue-accessible-color-picker": "^4.1.2",
    "vue-final-modal": "^3.4.11",
    "vue-flatpickr-component": "^11.0.3",
    "vue-i18n": "^9.2.2",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "peerDependencies": {
    "konva": "^8.0.0",
    "qrcode": "^1.5.0",
    "jsbarcode": "^3.11.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0",
    "terser": "^5.0.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prappo/labelbaker-creative-editor.git"
  },
  "bugs": {
    "url": "https://github.com/prappo/labelbaker-creative-editor/issues"
