{
  "name": "labelbaker-creative-editor",
  "version": "1.0.3",
  "description": "A powerful creative editor for labels and designs",
  "author": "Prappo",
  "license": "MIT",
  "keywords": [
    "editor",
    "label",
    "design",
    "creative",
    "canvas",
    "konva"
  ],
  "main": "dist/lib/editor.cjs.js",
  "module": "dist/lib/editor.es.js",
  "unpkg": "dist/lib/editor.umd.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib/editor.es.js",
      "require": "./dist/lib/editor.cjs.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:lib": "vite build --config vite.lib.config.js",
    "preview": "vite preview",
    "dbuild": "vite build --debug",
    "visualizer": "npx vite-bundle-visualizer",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "konva": "^8.0.0",
    "qrcode": "^1.5.0",
    "jsbarcode": "^3.11.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "vite": "^4.0.0",
    "terser": "^5.0.0",
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prappo/labelbaker-creative-editor.git"
  },
  "bugs": {
    "url": "https://github.com/prappo/labelbaker-creative-editor/issues"
  },
  "homepage": "https://prappo.github.io/labelbaker-creative-editor/"
}
